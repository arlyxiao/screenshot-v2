{
    "name": "screenshot-v2",	 
    "alias": "screenshot-v2.now.sh",
    "version": 2,
    "builds": [
        { "src": "favicon.ico", "use": "@now/static" },
        { "src": "screenshot.js", "use": "@now/node", "config": { "maxLambdaSize": "40mb" } }
    ],
    "routes": [
        { "src": "/favicon.ico", "dest": "/favicon.ico" },
        { "src": "/(.*)", "dest": "/screenshot.js" }
    ]
}