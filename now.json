{
    "name": "screenshot-v2",	 
    "alias": "screenshot-v2.now.sh",
    "version": 2,
    "builds": [
        { "src": "favicon.ico", "use": "@now/static" },
        { "src": "lambda.js", "use": "@now/node", "config": { "maxLambdaSize": "50mb", "bundle": false } }
    ],
    "routes": [
        { "src": "/favicon.ico", "dest": "/favicon.ico" },
        { "src": "/(.*)", "dest": "/lambda.js" }
    ]
}